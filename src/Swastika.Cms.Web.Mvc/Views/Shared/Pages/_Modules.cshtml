@using System.Globalization
@using Microsoft.AspNetCore.Http.Features
@using Microsoft.AspNetCore.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@model Swastika.Cms.Lib.ViewModels.CategoryFEViewModel

@foreach (var nav in Model.Modules.OrderBy(n => n.Priority))
{
    if (nav != null)
    {
        Html.RenderPartial(nav.Template, nav);
    }

}





























